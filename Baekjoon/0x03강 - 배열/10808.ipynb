   "source": [
    "alpha_dict = {chr(i): 0 for i in range(97, 123)}\n",
    "\n",
    "word = input().lower()\n",
    "\n",
    "for i in word:\n",
    "    if i in alpha_dict:\n",
    "        alpha_dict[i] += 1\n",
    "\n",
    "print(' '.join([str(alpha) for alpha in alpha_dict.values()]))"
   ]
  }
