   ],
   "source": [
    "N, K = map(int, input().split())\n",
    "ls = [i for i in range(1, N+1)]\n",
    "\n",
    "answer_list = []\n",
    "cnt = 0\n",
    "for i in range(N):\n",
    "    cnt += K - 1\n",
    "    if cnt >= len(ls):\n",
    "        cnt = cnt%len(ls)\n",
    "    answer_list.append(str(ls.pop(cnt)))\n",
    "\n",
    "print(f\"<{', '.join(answer_list)}>\")"
   ]
  }
